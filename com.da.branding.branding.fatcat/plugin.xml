<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.da.branding.example.application"
            name="FatCat">
         <property
               name="appName"
               value="Catastic">
         </property>
         <property
               name="windowImages"
               value="res/windowimage.gif">
         </property>
         <property
               name="aboutText"
               value="This is FatCat!">
         </property>
         <property
               name="aboutImage"
               value="res/about.gif">
         </property>
      </product>
   </extension>

</plugin>
